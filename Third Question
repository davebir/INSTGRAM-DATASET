-- WE WANT TO TARGET OUR INACTIVE USERS WITH AN EMAIL CAMPAIGN --
-- FIND THE USERS WHO HAVE NEVER POSTED A PHOTO BEFORE --



SELECT

  users.id,
  
  username,
  
FROM

  users
  
      LEFT JOIN
     photos ON photos.users_id = users.id
     
   WHERE
     photos.id IS NULL
     
   ORDER BY users.id;
   
   
   
